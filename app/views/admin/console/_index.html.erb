<%= osu.block_to_partial 'shared/modal',
                         html_id: 'admin_console_dialog',
                         width: '900px',
                         height: '500px' do %>

  <div class="modal-header" style="border-bottom: none">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
    <ul class="nav nav-tabs tight-bottom">
      <li class="active"><a href="#manage-tab" data-toggle="tab">Manage</a></li>
      <li><a href="#exceptions-tab" data-toggle="tab">Exceptions</a></li>
      <% unless Rails.env.production? %>
        <li class="dev">
          <a href="#dev-tab" data-toggle="tab" class="orange">Dev</a>
        </li>
      <% end %>
    </ul>
  </div>

  <div class="modal-body">

    <div id="dialog-local-alerts"></div>

    <div class="tab-content">
      <div class="tab-pane active" id="manage-tab">
        <%= render partial: 'admin/console/manage' %>
      </div>

      <div class="tab-pane" id="exceptions-tab">
        <%= render partial: 'admin/console/exceptions' %>
      </div>

      <% unless Rails.env.production? %>
        <div class="tab-pane" id="dev-tab">
          <%= render partial: 'dev/console/index' %>
        </div>
      <% end %>
    </div>

  </div>

<% end %>
